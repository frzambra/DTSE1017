elim_anomal <- function(x,...){
if (!is.numeric(x)) stop('suministre valor numérico')
lim_inf <- quantile(x,.25,...) - 1.5*IQR(x,...)
lim_sup <- quantile(x,.75,...) + 1.5*IQR(x,...)
x[x < lim_inf | x > lim_sup] <- NA
return(x)
}
